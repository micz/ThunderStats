  "InboxZeroTitle": {
    "message": "What is Inbox Zero?",
    "description": ""
  },
  "InboxZeroText_1": {
    "message": "Inbox Zero is a rigorous approach to email management aimed at keeping the inbox empty — or almost empty — at all times.",
    "description": ""
  },
  "InboxZeroText_2": {
    "message": "ThunderStats gives you some insights on how you're managing your inbox.",
    "description": ""
  },
  "OtherInfoTitle": {
    "message": "How does this addon works?",
    "description": ""
  },
  "OtherInfoText_1": {
    "message": "All the statistics are calculated from the internal Thunderbird database and are associated with the mail addresses of the selected account.",
    "description": ""
  },
  "OtherInfoText_2b": {
    "message": "The statistics will be updated as soon as the Thunderbird Indexer has processed all new messages. The dates in the top right corner show how up-to-date the statistics are.",
